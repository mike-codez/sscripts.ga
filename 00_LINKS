<hr></pre></body><script>
function linkURL(text) {
	var exp = /(\b((https?|ftp|file):\/\/|(www))[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|]*)/ig;
	var link = text.replace(exp,"<a href='$1'>$1</a>"); document.write(link.replace(/\n/g, '<br>'));
}</script></html>
