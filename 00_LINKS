function linkURL(text) {
	var exp = /(\b((https?|ftp|file):\/\/|(www))[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|]*)/gi;
	var link = text.replace(exp,"<a href='$1'>$1<\/a>"); document.writeln(link);
}</script></html>
